{
  "name": "circleso",
  "version": "0.2.0",
  "description": "Circleso async client",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atomimus/circleso.git"
  },
  "bugs": {
    "url": "https://github.com/atomimus/circleso/issues"
  },
  "homepage": "https://github.com/atomimus/circleso#readme",
  "keywords": [
    "circle.so",
    "circleso",
    "sdk",
    "api",
    "headless"
  ],
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./admin": {
      "types": "./dist/admin.d.ts",
      "import": "./dist/admin.js",
      "require": "./dist/admin.cjs"
    },
    "./headless": {
      "types": "./dist/headless.d.ts",
      "import": "./dist/headless.js",
      "require": "./dist/headless.cjs"
    },
    "./auth": {
      "types": "./dist/auth.d.ts",
      "import": "./dist/auth.js",
      "require": "./dist/auth.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "openapi-fetch": "0.15.0"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "format": "biome format --write .",
    "spec:sync": "tsx ./scripts/spec-sync.ts",
    "gen:types": "tsx ./scripts/gen-types.ts",
    "prepublishOnly": "npm run gen:types && npm run build"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@types/node": "25.0.3",
    "openapi-typescript": "7.10.1",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "tsx": "4.21.0",
    "vitest": "4.0.16"
  }
}
